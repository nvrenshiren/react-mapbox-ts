{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
      "react-mapbox-ts": ["react-mapbox-ts/src/index"]
    },
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true
  },
  "include": ["src", "types", "./typings.d.ts"]
}

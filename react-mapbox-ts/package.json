{
  "name": "react-mapbox-ts",
  "version": "1.0.16",
  "description": "Write mapbox with react and typescript",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:cleanup": "rimraf lib && rimraf lib-esm",
    "build:lib": "npm run build:cleanup && tsc",
    "build:standalone": "cross-env BABEL_ENV=production webpack",
    "build:esm": "tsc -p tsconfig-esm.json",
    "build": "npm run build:lib && npm run build:standalone&&npm run build:esm",
    "pub": "npm publish"
  },
  "homepage": "https://nvrenshiren.github.io/react-mapbox-ts-site/dist/index.html",
  "files": [
    "lib/**/*",
    "lib-esm/**/*"
  ],
  "author": "nvrenshiren",
  "license": "MIT",
  "peerDependencies": {
    "mapbox-gl": ">=1.10.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/mapbox-gl": "^1.9.1",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "cross-env": "^7.0.2",
    "mapbox-gl": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "@types/geojson": "^7946.0.7"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.1"
  },
  "keywords": [
    "mapbox",
    "react",
    "typescript",
    "mapbox-gl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nvrenshiren/react-mapbox-ts"
  }
}
